{
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended",
    "plugin:jest-formatting/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:prettier/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "plugins": ["import", "jest", "jest-formatting"],
  "env": {
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "rules": {
    "import/newline-after-import": ["error"]
  }
}
